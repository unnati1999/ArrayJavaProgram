public class Array{
    public static void main(String[]args)
    {
        int arr[]={12,18,14,6,4};
        for(int i=0;i<5;i++)
        {
            System.out.println(arr[i]);
        }
        System.out.println("reverse array");
        for(int i=arr.length-1;i>=0;i--)
        {
            System.out.println(arr[i]);
        }
    }
}
